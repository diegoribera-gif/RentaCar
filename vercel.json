{
  "builds": [
    {
      "src": "VercelApp/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "VercelApp/wsgi.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task"
  }
}